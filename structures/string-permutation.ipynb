    "def sp(a, l, r):\n",
    "    if l==r:\n",
    "        print(''.join(a))\n",
    "    else:\n",
    "        for i in range(l,r):\n",
    "            print(i, l, r)\n",
    "            a[l], a[i] = a[i], a[l]\n",
    "            sp(a, l+1, r)\n",
    "            a[l], a[i] = a[i], a[l] # backtrack\n",
    "sp(list(\"ABCD\"), 0, len(\"ABCD\"))"
   ]
